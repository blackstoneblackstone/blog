{
  "manifest_version": 2,
  "name": "Auto blog from YouDao",
  "description": "Auto blog from YouDao",
  "version": "1.0",
  "permissions":  [
    "activeTab",
    "storage",
    "tabs",
    "cookies",
    "proxy",
    "https://*/"
  ],
  "browser_action": {
    "default_title":"Auto blog from YouDao",
    "default_icon": "./favicon.png",
    "default_popup": "popup.html"
  }
}